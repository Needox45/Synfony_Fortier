{% extends 'home.html.twig' %}

{% block title %}Article List{% endblock %}

{% block main_body %}
<h1>Article List</h1>
<ul>
    {% for article in articles %}
        <li>
            <a href="{{ path('show_article', {id: article.id}) }}">{{ article.titre }}</a>
        </li>
    {% endfor %}
</ul>
{% endblock %}
